  },
  "config": {
    "webpack": "node_modules/laravel-mix/setup/webpack.config.js"
  },
  "dependencies": {
    "hellojs": "^1.16.1"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "laravel-mix": "^2.0.0"
  }
