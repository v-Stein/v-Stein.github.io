using @Console;


# Declaration of a class that inherits from native Console
class HelloWorld from Console {

    # Private property
    - String helloWorld;

    # Constructor
    + Construct () {
        this.helloWorld = "Hello, World!";
    }

    # Methods don't return any value, otherwise, it's a function
    + method hello () {
        # Calling
        this.out(this.helloWorld);
    }

    # Functions have to return a value, even if it's None
    + function getHello :String () {
        >> this.helloWorld;
    }

}

Console c = new Console();

# Calling Console::out(String) method
c.out("Hello, World! (from Console variable)");


HelloWorld hw = new HelloWorld();
# Calling HelloWorld::hello() method
hw.hello();

# Calling Console::out(String) method with HelloWorld::getHello():String function as parameter
c.out(hw.getHello());